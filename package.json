{
  "name": "ld2h",
  "version": "0.4.1",
  "description": "Expand tags by rendering local or remote RDF resources, recursively.",
  "main": "src/ld2h.js",
  "devDependencies": {
    "browserify": "^6.3.3",
    "es6-promise": "^3.2.1",
    "exorcist": "^0.4.0",
    "mocha-phantomjs": "^4.1.0"
  },
  "dependencies": {
    "rdf-ext": "0.3.0",
    "rdf-parser-n3": "0.3.0",
    "rdf2h": "0.3.0",
    "rdf-store-ldp": "0.3.1"
  },
  "scripts": {
    "build-test": "browserify test/test.js > test/test.build.js",
    "prepublish": "mkdir -p dist && browserify src/ld2h.js  -d --external rdf-ext --external rdf2h --external es6-promise  | exorcist dist/ld2h.js.map > dist/ld2h.js",
    "test": "npm run build-test && mocha-phantomjs test/index.html"
  },
  "repository": "https://github.com/rdf2h/ld2h.git",
  "keywords": [
    "rdf",
    "linked-data",
    "rendering"
  ],
  "author": "Reto Gm��r",
  "license": "Apache-2.0",
  "homepage": "http://rdf2h.github.io/ld2h/"
}
